{
    "manifest_version": 2,
  
    "name": "巴韭特",
    "description": "巴韭特",
    "version": "1.0",
  
    "icons": {
      "16" : "./static/leek.png",
      "48" : "./static/leek.png",
      "128" : "./static/leek.png"
    },
    "content_scripts": [
      {
        "js":["main.js"],
        "matches":["<all_urls>"]
      }
    ],
    "browser_action": {
      // "default_icon": "img/icon.png",
      // // 图标悬停时的标题，可选
      "default_title": "巴韭特",
      "default_popup": "index.html"
    },
    // 允许访问的域名
    "permissions": [
      "http://fundgz.1234567.com.cn/",
      "http://*/*",
      "https://*/*",
      "storage"
    ],
    "homepage_url": "https://www.baidu.com"
    // 注入js
    // "web_accessible_resources": [
    //   "index.js"
    // ]
  }